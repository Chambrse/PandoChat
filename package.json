{
  "name": "pandochat",
  "version": "0.1.0",
  "description": "A comprehensive, easy to use, high volume chat interface.",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node database/scripts/seed.js",
    "install": "cd client && yarn install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "color-convert": "^2.0.1",
    "concurrently": "^3.5.0",
    "nodemon": "^1.11.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "animate.css": "^3.7.0",
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "bootstrap-toggle": "^2.2.2",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^2.0.1",
    "connect-session-sequelize": "^5.2.2",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.2.0",
    "ejs": "^2.5.6",
    "express": "^4.17.3",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.15.6",
    "express-socket.io-session": "^1.3.5",
    "express-validator": "^5.3.0",
    "handlebars": "^4.0.11",
    "if-env": "^1.0.4",
    "jquery": "^3.3.1",
    "jquery.transit": "^0.9.12",
    "mongoose": "^5.3.7",
    "mysql2": "^1.6.1",
    "node-pre-gyp": "^0.11.0",
    "nodemailer": "^6.4.6",
    "npm": "^6.4.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "project-name-generator": "^2.1.7",
    "random-sentence": "^1.0.4",
    "randomcolor": "^0.5.4",
    "react-adsense": "^0.1.0",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-color": "^2.17.3",
    "sequelize": "^4.38.0",
    "serve-favicon": "^2.5.0",
    "socket.io": "^1.7.3",
    "validator": "^10.8.0",
    "velocity-react": "^1.4.1",
    "yarn": "^1.22.0"
  }
}
